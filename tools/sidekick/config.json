{
  "project": "My Screen Project",
  "host": "dx-recognitions.aem-screens.net",
  "previewUrl": "dx-recognitions-prev.aem-screens.net",
  "plugins": [
    {
      "id": "publish",
      "excludePaths": [ "**" ],
      "environments": ["preview"]
    },
    {
      "id": "publish-channel-doc",
      "title": "Publish",
      "event": "publish-channel",
      "passConfig": true,
      "passReferrer": true,
      "environments": ["preview"]
    },
    {
      "id": "preview-dashboard",
        "title": "Preview-Dashboard",
        "event": "preview-dashboard",
        "passConfig": true,
        "passReferrer": true,
        "environments": ["preview"]
    }
  ]
}
