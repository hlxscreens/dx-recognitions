{
  "project": "My Screen Project",
  "host": "dx-recognitions.aem-screens.net",
  "previewUrl": "dx-recognitions-prev.aem-screens.net",
  "plugins": [
    {
      "id": "edit-preview",
      "excludePaths": [ "**spreadsheets**" ]
    },
    {
      "id": "preview-sheet",
      "title": "Preview Sheet",
      "passConfig": true,
      "passReferrer": true,
      "url": "/tools/preview-sheet/index.html",
      "excludePaths": [ "**" ],
      "includePaths": [ "**spreadsheets**" ]
    },
    {
      "id": "publish-channel",
      "title": "Publish Channel",
      "passConfig": true,
      "passReferrer": true,
      "url": "/tools/publish-sheet/index.html",
      "excludePaths": [
        "**"
      ],
      "includePaths": [
        "**spreadsheets**"
      ]
    },
    {
      "id": "publish-channel-doc",
      "title": "Publish Channel",
      "event": "publish-channel",
      "passConfig": true,
      "passReferrer": true,
      "environments": ["preview"]
    }
  ]
}
